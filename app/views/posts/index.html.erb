<div class="row">
  <h1 class="text-center">
    Time Entries
  </h1>
    <%= link_to 'New Post', new_post_path, class: "btn btn-sm btn-primary pull-right" %>
</div>
<div class="row">
  <table class="table table-striped">
      <thead>
          <tr>
            <th>#</th>
            <th>Overtime Requested</th>
            <th>Date</th>
            <th>User</th>
            <th>Rationale</th>
            <th>Status</th>
            <th></th>
            <th></th>
          </tr>
      </thead>
      <tbody>
        <%= render @posts %>
      </tbody>

  </table>
</div>

<div class="row">
  <div class="text-center">
    <%= paginate @posts %>
  </div>
</div>
