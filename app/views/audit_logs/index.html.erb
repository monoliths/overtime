<div class="row">
  <h1 class="text-center">
    Audit Logs
  </h1>
  <div class="pull-right">
    <%= paginate @audit_logs %>
  </div>
</div>

<div class="row">
  <table class="table table-striped">
      <thead>
          <tr>
            <th>#</th>
            <th>Employee</th>
            <th>Week Starting</th>
            <th>Confirmed At</th>
            <th>Status</th>
          </tr>
      </thead>
      <tbody>
        <%= render @audit_logs %>
      </tbody>

  </table>
</div>
